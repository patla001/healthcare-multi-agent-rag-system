{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "agent-python-backend/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/python/(.*)",
      "dest": "agent-python-backend/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11",
    "NEXT_PUBLIC_BACKEND_URL": ""
  },
  "functions": {
    "agent-python-backend/main.py": {
      "runtime": "python3.11"
    },
    "api/python.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/python.py"
    }
  ],
  "build": {
    "env": {
      "NEXT_PUBLIC_BACKEND_URL": ""
    }
  }
} 